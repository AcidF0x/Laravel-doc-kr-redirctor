{
  "name" : "Laravel document KR Redirector",
  "version" : "0.0.1",
  "description" : "Laravel 영문 문서 접속시 한국어 페이지로 자동 전환 해줍니다",
  "icons": {
    "16":  "icon/default-16.png",
    "32":  "icon/default-32.png",
    "48":  "icon/default-48.png",
    "128": "icon/default-128.png"
  },
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "browser_action": {
    "default_title": "Active/inactive Redirect",
    "default_icon": "icon/default-48.png",
    "default_popup": "index.html"
  }
}